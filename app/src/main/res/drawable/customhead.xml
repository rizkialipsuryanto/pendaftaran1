<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <layer-list android:opacity="transparent">
            <item>
                <shape>
                    <solid android:color="@color/colorhead" />
                    <corners android:topRightRadius="10dp" android:topLeftRadius="10dp"/>
                    <padding android:right="5dp" android:left="5dp" android:top="5dp" android:bottom="5dp"/>
                    <stroke
                        android:color="@color/colorhead"
                        android:width="1dp" />
                </shape>
            </item>
        </layer-list>
    </item>
</selector>
